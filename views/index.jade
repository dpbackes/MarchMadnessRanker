extends layout

block content
	h1= title
	p Welcome to #{title}
	table
		tr 
			td Name
			td Sagarin Rank
			td Massey Rank
		- each team in teams
			tr
				td #{team.name}
				- if(team.Sagarin)
					td #{team.Sagarin.rank}
				- else
					td
				- if(team.Massey)
					td #{team.Massey.rank}
				- else
					td
	